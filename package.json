{
	"name": "slug-aid",
	"private": true,
	"scripts": {
		"build": "dotenv -e .env turbo build",
		"dev": "dotenv -e .env turbo dev",
		"lint": "dotenv -e .env turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"dotenv-cli": "^8.0.0",
		"prettier": "^3.2.5",
		"turbo": "^2.3.3",
		"typescript": "5.5.4"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@10.9.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"dotenv": "^16.4.7"
	}
}
